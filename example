// AFTER (lint-safe, same functionality)
const contactListToDelete = contactObject.filter(function (c) {
  if (c.FilterKey === keyToFilter) {
    // ✅ use forEach for side-effects
    (Array.isArray(c.contacts) ? c.contacts : []).forEach(function (localContact) {
      const type = util.replaceNullorEmpty(localContact && localContact.contactType).toUpperCase();
      if (type === Constants.CONTACT_TYPES_MANDATORY_RULE.ADMIN1) {
        isAdmin1ToDelete = true;
        admin1ContactToDelete = localContact && localContact.contactId;
      }
    });
    return true;    // ✅ explicit boolean for filter
  }
  return false;     // ✅ explicit boolean for filter
});