const handleSave = async () => {
  const payload = {
    Name: form.name,
    Service_Type: form.serviceType,
    Trustee_ID: parseInt(form.trusteeId)
  };

  if (editId) {
    await api.put(`/sp/${editId}`, payload);
  } else {
    await api.post("/sp", payload);
  }

  fetchSP();
  setOpen(false);
  setForm({ name: "", serviceType: "", trusteeId: "" });
  setEditId(null);
};