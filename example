<div style={styles.toggleSwitchesDiv} id="contactTypeDivId">
  {contactTypes.map((eachContactType, i) => {
    const contactTypeId = eachContactType.contactTypeId;
    const toggleSwitch = this.state.toggleSwitch;
    const toggleSwitchRules = this.state.toggleSwitchRules;

    // decide disabled state
    const toggleDisabled = !this.props.AddExisting
      ? !!toggleSwitchRules[contactTypeId]
      : false;

    // (optional) skip rendering certain IDs
    // if ([Constants.ROLESREGION.AGENTBROKER, ...].includes(contactTypeId)) return null;

    return (
      <div key={contactTypeId ?? i} className="contact-type-row">
        <span>{contactTypeId}</span>
        <ToggleSwitch
          checked={!!toggleSwitch[contactTypeId]}
          disabled={toggleDisabled}
          onChange={(val) => this.handleToggleValue(val, contactTypeId)}
        />
      </div>
    );
  })}
</div>